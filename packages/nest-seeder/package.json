{
  "name": "@ackplus/nest-seeder",
  "version": "1.1.15-beta.3",
  "description": "A powerful and flexible database seeding library for NestJS applications with support for factories, data generation using Faker.js, and CLI commands",
  "author": "AckPlus",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nest-seed": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "QUICKSTART.md",
    "LICENSE"
  ],
  "keywords": [
    "nestjs",
    "seeder",
    "database",
    "seed",
    "faker",
    "factory",
    "typeorm",
    "mongoose",
    "prisma",
    "data-generation",
    "test-data",
    "fixtures"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ack-solutions/nest-seeder"
  },
  "bugs": {
    "url": "https://github.com/ack-solutions/nest-seeder/issues"
  },
  "homepage": "https://github.com/ack-solutions/nest-seeder#readme",
  "scripts": {
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "build": "pnpm clean && pnpm -C ../.. exec tsc -p packages/nest-seeder/tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "@faker-js/faker": "^9.0.0",
    "reflect-metadata": "^0.2.2"
  },
  "peerDependenciesMeta": {
    "ts-node": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
